{
  "name": "mscrtc",
  "version": "1.5.0",
  "summary": "MSCRTC - WebRTC functions for mscrtc-ios",
  "description": "This pod add WebRTC functions to an app. \nIt works together with the msc-kurento mediaserver implementation in Java",
  "homepage": "https://github.com/inspiraluna/AppRTC-iOS",
  "license": "MIT",
  "authors": {
    "Nico Krause": "nico@le-space.de"
  },
  "source": {
    "git": "https://github.com/inspiraluna/AppRTC-iOS.git",
    "tag": "1.5.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "dependencies": {
    "SocketRocket": [

    ]
  },
  "source_files": "Pod/Classes",
  "vendored_frameworks": "WebRTC.framework",
  "public_header_files": [
    "Pod/Classes/**/*.h",
    "WebRTC.framework/Headers/*.h"
  ],
  "resource_bundles": {
    "mscrtc": [
      "Pod/**/*.{png,jpeg,jpg,lproj,storyboard,xcassets}"
    ]
  },
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/WebRTC.framework\"",
    "LD_RUNPATH_SEARCH_PATHS": "@loader_path/../Frameworks"
  },
  "requires_arc": true
}
